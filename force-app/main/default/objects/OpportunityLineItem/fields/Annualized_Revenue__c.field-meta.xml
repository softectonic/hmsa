<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Annualized_Revenue__c</fullName>
    <externalId>false</externalId>
    <formula>UnitPrice  * (IF ( PricebookEntry.Product2.ProductCode = &#39;TDI&#39;, 4, 12))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Calculated the Annualized revenue by multiplying Sales Price by 4 for Product Code of &quot;TDI&quot; and by 12 for all other products.</inlineHelpText>
    <label>Annualized Revenue</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <type>Currency</type>
</CustomField>
