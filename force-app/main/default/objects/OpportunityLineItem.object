<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <fields>
        <fullName>Annualized_Revenue__c</fullName>
        <externalId>false</externalId>
        <formula>UnitPrice  * (IF ( PricebookEntry.Product2.ProductCode = &apos;TDI&apos;, 4, 12))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Calculated the Annualized revenue by multiplying Sales Price by 4 for Product Code of &quot;TDI&quot; and by 12 for all other products.</inlineHelpText>
        <label>Annualized Revenue</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Annualized_Revenue_for_Rollup__c</fullName>
        <description>Hidden, stored field to be used in Rollup Summary.  Set via Process Builder.</description>
        <externalId>false</externalId>
        <label>Annualized Revenue for Rollup</label>
        <precision>9</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Billing_Group__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <externalId>false</externalId>
        <label>Billing Group</label>
        <referenceTo>Billing_Group__c</referenceTo>
        <relationshipLabel>Opportunity Product</relationshipLabel>
        <relationshipName>Opportunity_Product</relationshipName>
        <required>false</required>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Complementary_Coverage_Code__c</fullName>
