    </fields>
    <fields>
        <fullName>Missing_Required_Revenue__c</fullName>
        <externalId>false</externalId>
        <formula>Selected_By_Customer__c &amp;&amp; 
 UnitPrice  = 0 &amp;&amp;
(
PricebookEntry.Product2.Name  = &apos;Plan Offering&apos; &amp;&amp; NOT ISBLANK ( 
Dental_Coverage_Code__c ) 
|| 
PricebookEntry.Product2.Name &lt;&gt; &apos;Plan Offering&apos; 
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Missing Required Revenue</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Missing_Required_Revenue_for_Rollup__c</fullName>
        <defaultValue>false</defaultValue>
        <externalId>false</externalId>
        <label>Missing Required Revenue for Rollup</label>
        <type>Checkbox</type>
    </fields>
